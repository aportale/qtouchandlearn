<?xml version='1.0' encoding='utf-8' standalone='no'?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>com.aportale.touchandlearn</id>
    <name>touchandlearn</name>
    <versionNumber>1.0.0</versionNumber>
    <description>DESCRIPTION</description>
    <initialWindow>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
        <autoOrients>true</autoOrients>
        <aspectRatio>landscape</aspectRatio>
    </initialWindow>
    <publisher>PUBLISHER</publisher>
    <copyright>COPYRIGHT</copyright>
    <env value="app/native/imports" var="QML_IMPORT_PATH"/>
    <env value="app/native/plugins" var="QT_PLUGIN_PATH"/>
    <env value="app/native/lib" var="LD_LIBRARY_PATH"/>
    <arg>-platform</arg>
    <arg>qnx</arg>
    <arg>-style</arg>
    <arg>qnxlight</arg>
    <action system="true">run_native</action>
    <!-- PROJECTPATH should point to the project binary, path can be relative -->
    <asset path="touchandlearn" type="Qnx/Elf" entry="true">touchandlearn</asset>
    <!-- These values will be replaced when deploying in Qt Creator -->
    <asset path="%QT_INSTALL_LIBS%/libQt5Core.so.5">lib/libQt5Core.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Network.so.5">lib/libQt5Network.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Multimedia.so.5">lib/libQt5Multimedia.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Gui.so.5">lib/libQt5Gui.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Qml.so.5">lib/libQt5Qml.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Quick.so.5">lib/libQt5Quick.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5V8.so.5">lib/libQt5V8.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Xml.so.5">lib/libQt5Xml.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Sql.so.5">lib/libQt5Sql.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5QuickParticles.so.5">lib/libQt5QuickParticles.so.5</asset>
    <asset path="%QT_INSTALL_PLUGINS%/sqldrivers">plugins/sqldrivers</asset>
    <asset path="%QT_INSTALL_PLUGINS%/playlistformats">plugins/playlistformats</asset>
    <asset path="%QT_INSTALL_PLUGINS%/platforms">plugins/platforms</asset>
    <asset path="%QT_INSTALL_PLUGINS%/mediaservice">plugins/mediaservice</asset>
    <asset path="%QT_INSTALL_PLUGINS%/imageformats">plugins/imageformats</asset>
    <asset path="%QT_INSTALL_PLUGINS%/bearer">plugins/bearer</asset>
    <asset path="%QT_INSTALL_PLUGINS%/accessible">plugins/accessible</asset>
    <asset path="%QT_INSTALL_QML%/QtQuick.2">imports/QtQuick.2</asset>
    <asset path="%QT_INSTALL_QML%/QtQuick">imports/QtQuick</asset>
    <asset path="%QT_INSTALL_QML%/QtMultimedia">imports/QtMultimedia</asset>
    <asset path="%QT_INSTALL_QML%/QtAudioEngine">imports/QtAudioEngine</asset>
    <asset path="%QT_INSTALL_QML%/Qt">imports/Qt</asset>
    <asset path="%SRC_DIR%/touchandlearn64.png">touchandlearn64.png</asset>
    <asset path="%SRC_DIR%/qml">qml</asset>
    <asset path="%SRC_DIR%/data">data</asset>
    <asset path="%SRC_DIR%/mp3audio">mp3audio</asset>
    <icon>
        <image>touchandlearn64.png</image>
    </icon>
</qnx>
